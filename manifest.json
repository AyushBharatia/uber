{
    "manifest_version": 3,
    "name": "Random Address Generator",
    "version": "1.1",
    "description": "Automatically adds a regenerate button to generate a random address for specific input fields",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://maps.googleapis.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  